# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000

# Supabase Configuration (for getting auth tokens)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google OAuth Client IDs (optional)
# If you set at least one of these, the app will prefer the token flow:
# Google id_token -> Supabase signInWithIdToken (avoids showing the Supabase callback URL).
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
# Optional (mainly for Expo Go / generic clients)
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=
